{
  "name": "readyshop",
  "version": "1.0.0",
  "description": "eCommere app built with MERN stack",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon",
    "build": "etsc",
    "watch": "etsc --w",
    "test": "jest --coverage",
    "lint": "etsc --noEmit && eslint \"**/*.ts\" --fix",
    "lint:check": "eslint \"**/**/*.ts\"",
    "format:check": "prettier --check */**/*.*",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.7.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "esbuild": "0.19.3",
    "esbuild-node-tsc": "^2.0.5",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 120,
    "trailingComma": "all"
  }
}
